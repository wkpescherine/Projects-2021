<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:gravity="center">

        <!--  Sin/Cos/Tan  -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:visibility="invisible"
            android:id="@+id/sincostan"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/SCT"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="SCT"
                android:textSize="32dp"
                android:textColor="#fff"
                />
            <TextView
                android:id="@+id/Degree"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="degree"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusDegree"
                />
            <TextView
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"
                />
            <TextView
                android:id="@+id/radians"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:text="radians"
                android:textSize="32dp"
                android:textColor="#fff"
                />
        </LinearLayout>
        <!--  Add/Subtract/multiply/Divide  -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/Basic">
            <TextView
                android:id="@+id/X"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="x"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="setFocus"/>
            <TextView
                android:id="@+id/sign1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/Y"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="y"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="setFocus"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/Z"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="z"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="setFocus"/>
        </LinearLayout>
        <!--  Square/Cube  -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:id="@+id/SquareCube">
            <TextView
                android:id="@+id/H"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="h"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="setFocus"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/W"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="w"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="setFocus"/>
            <TextView
                android:id="@+id/sign2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="*"
                android:textSize="32dp"
                android:textColor="#fff"
                android:visibility="gone"/>
            <TextView
                android:id="@+id/D"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="d"
                android:textSize="32dp"
                android:textColor="#fff"
                android:visibility="gone"
                android:onClick="setFocus"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/Totvalue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="setFocus"/>
        </LinearLayout>
        <!--  Exponent  -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/ExponentFormula">
            <TextView
                android:id="@+id/expValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="x"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusEXP"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <TextView
                    android:id="@+id/expSuper"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="exp"
                    android:textSize="10dp"
                    android:textColor="#fff"
                    android:onClick="focusEXPSUP"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="none"
                    android:textSize="10dp"/>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/ExpTotvalue2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total"
                android:textSize="32dp"
                android:textColor="#fff"/>
        </LinearLayout>
        <!--  Custom  -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/CustomFormula">
            <TextView
                android:id="@+id/Custom"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/CusTotvalue2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total"
                android:textSize="32dp"
                android:textColor="#fff"/>
        </LinearLayout>
        <!--  Circle Area  -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/CircleFormula">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="\u03C0"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/Radius"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="r"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusRadius"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/CircleTotvalue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Total"
                android:textSize="32dp"
                android:textColor="#fff"/>
        </LinearLayout>
        <!-- Linear Velocity -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/LinearVelocity">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/intSpeed"
                android:text="si"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusSpeedI"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/finalSpeed"
                android:text="sf"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusSpeedF"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="/"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/time"
                android:text="t"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusTime"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/linearVelocity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="v"
                android:textSize="32dp"
                android:textColor="#fff"/>
        </LinearLayout>
        <!-- Pythagoren -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/Pythagoren">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Avalue"
                android:text="a"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusA"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <TextView
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:textSize="8dp"
                    android:textColor="#fff"/>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/Bvalue"
                android:text="b"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusB"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent">
                <TextView
                    android:layout_marginTop="10dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="2"
                    android:textSize="8dp"
                    android:textColor="#fff"/>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/Cvalue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="c"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusC"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="2"
                android:textSize="8dp"
                android:textColor="#fff"/>
        </LinearLayout>
        <!-- Angular Acceleration -->
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="invisible"
            android:layout_centerInParent="true"
            android:id="@+id/AngularAcceleration">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/angle1"
                android:text="\u03B8"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusAngle1"/>
            <TextView
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="i"
                android:textSize="8dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="-"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/angle2"
                android:text="\u03B8"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusAngle2"/>
            <TextView
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="f"
                android:textSize="8dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="/"
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/time2"
                android:text="t"
                android:textSize="32dp"
                android:textColor="#fff"
                android:onClick="focusTime2"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="="
                android:textSize="32dp"
                android:textColor="#fff"/>
            <TextView
                android:id="@+id/AngMotionAcc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="acc"
                android:textSize="32dp"
                android:textColor="#fff"/>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout><!--  479 -->