<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableRow
                android:id="@+id/row1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/val1"
                    android:text="1"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num1"/>
                <TextView
                    android:text="2"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num2"/>
                <TextView
                    android:text="3"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num3"/>
                <TextView
                    android:text="4"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num4"/>
                <TextView
                    android:id="@+id/val5"
                    android:text="5"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num5"/>
                <TextView
                    android:text="6"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num6"/>
            </TableRow>
            <TableRow
                android:id="@+id/row2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="7"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num7"/>
                <TextView
                    android:text="8"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num8"/>
                <TextView
                    android:text="9"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num9"/>
                <TextView
                    android:text="0"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="num0"/>
                <TextView
                    android:text="DEL"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="del"/>
                <TextView
                    android:text="("
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="openBracket"/>
            </TableRow>
            <TableRow
                android:id="@+id/row3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="/"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp" />
                <TextView
                    android:text="."
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="decimal"/>
                <TextView
                    android:text="+"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="plus"/>
                <TextView
                    android:text="-"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="minus"/>
                <TextView
                    android:text="*"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="multi"/>
                <TextView
                    android:text=")"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:textColor="#fff"
                    android:gravity="center"
                    android:textSize="32dp"
                    android:onClick="closeBracket"/>
            </TableRow>
        </TableLayout>
    </LinearLayout>

</LinearLayout><!-- 176 -->